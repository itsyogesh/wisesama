{
  "name": "@wisesama/types",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "devDependencies": {
    "@wisesama/config": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
